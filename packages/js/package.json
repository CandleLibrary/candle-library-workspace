{
    "name": "@candlelib/js",
    "version": "0.5.24",
    "description": "Javascript Parser",
    "main": "./build/library/javascript.js",
    "types": "./build/types/javascript.d.ts",
    "type": "module",
    "scripts": {
        "prepublishOnly": "rm -rf ./build && tsc & sleep 10",
        "test": "candle.cure ./test/",
        "test.watch": "candle.cure -w ./test/candlelibrary.js.spec.js",
        "build": "tsc",
        "compile": "./scripts/compile.sh"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/candlelibrary/js.git"
    },
    "files": [
        "/build",
        "/README.md",
        "/CHANGELOG.md",
        "/LICENSE"
    ],
    "keywords": [
        "ECMASCRIPT",
        "ecma",
        "javascript",
        "Parser"
    ],
    "author": "Anthony Weathersby",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/candlelibrary/js/issues"
    },
    "homepage": "https://github.com/candlelibrary/js#readme",
    "dependencies": {
        "@candlelib/conflagrate": "*",
        "@candlelib/hydrocarbon": "*",
        "@candlelib/uri": "*",
        "@candlelib/wind": "*"
    },
    "devDependencies": {
        "@candlelib/cure": "*",
        "@types/node": "^16.4.13"
    },
    "jshintConfig": {
        "esversion": 6,
        "unused": true
    }
}
